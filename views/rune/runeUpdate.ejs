<%- include("../includes/header.ejs", {title: "Update " + rune.page_name}); %>

<h1>Update <%= rune.page_name%></h1>
<form action="/rune/update" method="get">
    <input type="hidden" id="rune_id" name="rune_id" value="<%= rune.rune_id %>">
    Page name: <input id="page_name" name="page_name" type="text" />
    <br />
    <br />

    Attached to account_id: <input id="account_id" name="account_id" type="number" />
    <br />
    <br />

    Mark: <input id="mark" name="mark" type = "text" />
    <br />
    <br />
    Seal: <input id="seal" name="seal" type = "text" />
    <br />
    <br />
    Glyph: <input id="glyph" name="glyph" type = "text" />
    <br />
    <br />
    Quintessence: <input id="quintessence" name="quintessence" type = "text" />
    <br />
    <br />

    <input type="submit" />
    <br />

</form>

<%- include("../includes/footer.ejs"); %>